<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cdlr\codeBundle\Entity\Regions" table="regions">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="Nom" length="20" nullable="false"/>
    <field name="longitude" type="float" column="Longitude" precision="10" scale="0" nullable="false"/>
    <field name="laltitude" type="float" column="Laltitude" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
