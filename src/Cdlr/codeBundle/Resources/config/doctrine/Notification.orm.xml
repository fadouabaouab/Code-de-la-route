<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cdlr\codeBundle\Entity\Notification" table="notification">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="message" type="string" column="message" length="500" nullable="false"/>
    <field name="time" type="string" column="time" length="30" nullable="false"/>
    <field name="idDestination" type="integer" column="id_destination" nullable="false"/>
    <field name="typeDestination" type="string" column="type_destination" length="20" nullable="false"/>
    <field name="idSource" type="integer" column="id_source" nullable="false"/>
  </entity>
</doctrine-mapping>
